{
  "remote_cache": {
    "enabled": true,
    "bucket": "velocity-cache-mvp-public",
    "region": "us-east-1"
  },
  "scripts": {
    "build": {
      "command": "npm run build",
      "inputs": [
        "app/**/*",
        "src/**/*",
        "public/**/*",
        "package.json",
        "package-lock.json",
        "tsconfig.json",
        "next.config.js"
      ],
      "outputs": [
        ".next/"
      ],
      "env_keys": [
        "NODE_ENV"
      ]
    }
  }
}
